<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.studyParty.dubboApi.Mapper.GroupTaskAnswerMapper">
    <select id="selectUserGroupTaskAnswer" resultType="com.studyParty.entity.group.GroupTaskAnswer">
        select *
        from group_task_answer
        where user_id = #{userId}
    </select>
</mapper>